{
  "name": "vue-global-config",
  "PascalCasedName": "VueGlobalConfig",
  "version": "0.3.1",
  "private": false,
  "packageManager": "pnpm@7.18.2",
  "description": "Configure your Vue 2.6/2.7/3 components globally.",
  "author": "cloydlau",
  "license": "MIT",
  "repository": "https://github.com/cloydlau/vue-global-config",
  "keywords": [
    "global config",
    "global registration",
    "global configuration",
    "global options",
    "vue-demi",
    "vue",
    "component",
    "sfc"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/vue-global-config.umd.js",
      "import": "./dist/vue-global-config.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/vue-global-config.umd.js",
  "module": "./dist/vue-global-config.mjs",
  "unpkg": "./dist/vue-global-config.umd.js",
  "jsdelivr": "./dist/vue-global-config.umd.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "dev3": "cl switchVue 3 && vite --config ./demo/vue3/vite.config.ts --open",
    "dev2.7": "cl switchVue 2.7 && vite --config ./demo/vue2.7/vite.config.ts --open",
    "dev2.6": "cl switchVue 2.6 && vite --config ./demo/vue2.6/vite.config.ts --open",
    "serve": "vite preview",
    "doc": "vitepress dev --open /README",
    "build": "vite build",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "release": "npx case-police --fix && npx lint-staged && vitest run && pnpm build && cl release"
  },
  "peerDependencies": {
    "@vue/composition-api": ">=1",
    "vue": "2 || 3"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "change-case": "^4.1.2",
    "lodash-es": "^4.17.21",
    "vue-demi": "^0.13.11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.34.0",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.17",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/coverage-c8": "^0.25.8",
    "@vue/compiler-sfc": "^3.2.45",
    "@vue/test-utils": "^2.2.6",
    "case-police": "^0.5.10",
    "element-plus": "^2.2.27",
    "eslint": "^8.30.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "typescript": "^4.9.4",
    "unplugin-auto-import": "^0.12.1",
    "vite": "^4.0.2",
    "vite-plugin-dts": "^1.7.1",
    "vitepress": "1.0.0-alpha.32",
    "vitest": "^0.25.8",
    "vue": "^3.2.45",
    "vue-global-config": "^0.3.0"
  },
  "lint-staged": {
    "{src,__tests__}/**.*": [
      "eslint --fix"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
