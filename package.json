{
  "name": "vue-global-config",
  "version": "0.0.2",
  "author": "cloydlau",
  "description": "Configure your Vue 2 & 3 Components globally",
  "keywords": [
    "global config",
    "global configuration",
    "global options",
    "vue",
    "component",
    "sfc"
  ],
  "private": false,
  "license": "MIT",
  "main": "./dist/vue-global-config.umd.js",
  "module": "./dist/vue-global-config.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue-global-config.es.js",
      "require": "./dist/vue-global-config.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "clean": "rd /s /q .\\node_modules",
    "ri": "npm run clean && pnpm i",
    "u": "node scripts/upgrade.js",
    "r": "node scripts/release.js",
    "rs": "node scripts/release.js --straightforward",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "eslint",
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "change-case": "latest",
    "kayran": "latest",
    "lodash-es": "latest",
    "vue-demi": "latest"
  },
  "peerDependencies": {
    "vue": "^2.0.0 || >=3.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "chalk": "^4.1.2",
    "enquirer": "latest",
    "eslint": "latest",
    "eslint-config-prettier": "latest",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-vue": "latest",
    "execa": "^4.1.0",
    "husky": "latest",
    "lint-staged": "latest",
    "minimist": "latest",
    "prettier": "latest",
    "semver": "latest",
    "vite": "latest",
    "vue": "latest"
  }
}
