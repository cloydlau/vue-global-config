{
  "name": "vue-global-config",
  "version": "0.2.2",
  "private": false,
  "description": "Configure your Vue 2.6 / 2.7 / 3 components globally.",
  "author": "cloydlau",
  "license": "MIT",
  "repository": "https://github.com/cloydlau/vue-global-config",
  "keywords": [
    "global config",
    "global registration",
    "global configuration",
    "global options",
    "vue",
    "component",
    "sfc"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/vue-global-config.umd.js",
      "import": "./dist/vue-global-config.mjs",
      "browser": "./dist/vue-global-config.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/vue-global-config.umd.js",
  "module": "./dist/vue-global-config.mjs",
  "unpkg": "./dist/vue-global-config.umd.js",
  "jsdelivr": "./dist/vue-global-config.umd.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "dev3": "cl switchVue 3 && vite --config ./demo/vue3/vite.config.ts --open",
    "dev2.7": "cl switchVue 2.7 && vite --config ./demo/vue2.7/vite.config.ts --open",
    "dev2.6": "cl switchVue 2.6 && vite --config ./demo/vue2.6/vite.config.ts --open",
    "serve": "vite preview",
    "build": "vite build",
    "release": "npx case-police --fix && npx lint-staged && cl switchVue 3 && pnpm build && cl release"
  },
  "peerDependencies": {
    "@vue/composition-api": "1",
    "vue": "2 || 3"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "change-case": "latest",
    "lodash-es": "latest",
    "vue-demi": "latest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "latest",
    "@vitejs/plugin-vue": "latest",
    "@vue/compiler-sfc": "latest",
    "@vue/test-utils": "latest",
    "case-police": "latest",
    "element-plus": "latest",
    "eslint": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "typescript": "latest",
    "unplugin-auto-import": "latest",
    "vite": "latest",
    "vite-plugin-dts": "latest",
    "vue": "latest",
    "vue-global-config": "latest"
  },
  "lint-staged": {
    "!(demo/**/*)*.{js,ts,tsx,vue,md}": [
      "eslint --fix"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
