{
  "name": "vue-global-config",
  "PascalCasedName": "VueGlobalConfig",
  "version": "0.4.0",
  "private": false,
  "packageManager": "pnpm@7.25.0",
  "description": "Configure your Vue 2.6/2.7/3 components globally.",
  "author": "cloydlau",
  "license": "MIT",
  "repository": "https://github.com/cloydlau/vue-global-config",
  "keywords": [
    "vue",
    "vue-demi",
    "global config",
    "global configuration",
    "global option",
    "global options",
    "global slot",
    "global slots"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/vue-global-config.umd.js",
      "import": "./dist/vue-global-config.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/vue-global-config.umd.js",
  "module": "./dist/vue-global-config.mjs",
  "unpkg": "./dist/vue-global-config.umd.js",
  "jsdelivr": "./dist/vue-global-config.umd.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "dev3": "cl switchVue 3 && vite --config ./demo/vue3/vite.config.ts --open",
    "dev2.7": "cl switchVue 2.7 && vite --config ./demo/vue2.7/vite.config.ts --open",
    "dev2.6": "cl switchVue 2.6 && vite --config ./demo/vue2.6/vite.config.ts --open",
    "doc": "vitepress dev --open /README",
    "build": "vite build",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "release": "npx lint-staged && vitest run && pnpm build && cl release"
  },
  "peerDependencies": {
    "@vue/composition-api": ">=1",
    "vue": "2||3"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "change-case": "latest",
    "lodash-es": "latest",
    "vue-demi": "latest"
  },
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "@types/lodash-es": "latest",
    "@types/node": "latest",
    "@vitejs/plugin-vue": "latest",
    "@vitest/coverage-c8": "latest",
    "@vue/compiler-sfc": "latest",
    "@vue/test-utils": "latest",
    "case-police": "latest",
    "element-plus": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "rome": "latest",
    "typescript": "latest",
    "unplugin-auto-import": "latest",
    "vite": "latest",
    "vite-plugin-dts": "latest",
    "vitepress": "latest",
    "vitest": "latest",
    "vue": "latest",
    "vue-global-config": "latest"
  },
  "lint-staged": {
    "{src,__tests__}/**.*": "rome check",
    "*.md": "case-police --fix"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
